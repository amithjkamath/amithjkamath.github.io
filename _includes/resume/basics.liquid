<table class="table table-cv table-sm table-borderless table-responsive table-cv-map">
  {% assign skip_basics = 'name,label,image,profiles,location,url,phone' | split: ',' %}
  {% for content in data[1] %}
    {%
      if (content[1] == "") or (skip_basics contains
      content[0])
    %}
      {% continue %}
    {% endif %}

    <tr>
      <td class="p-1 font-weight-light text" colspan="2">
        {% if content[0] == 'email' %}
          <a href="mailto:{{ content[1] }}" target="_blank">{{ content[1] }}</a>
        {% else %}
          {{ content[1] }}
        {% endif %}
      </td>
    </tr>
  {% endfor %}
</table>
